<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>上乘生活</name>
    <description>
        Hello World sample application that responds to the deviceready event.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <feature name="http://api.phonegap.com/1.0/device" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
    <preference name="SplashScreen" value="splash" />
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="EnableViewportScale" value="true" />
    <icon src="icon.png" />
    <icon gap:density="ldpi" gap:platform="android" src="res/icon/android/icon.png" />
    <icon gap:density="mdpi" gap:platform="android" src="res/icon/android/icon.png" />
    <icon gap:density="hdpi" gap:platform="android" src="res/icon/android/icon.png" />
    <icon gap:density="xhdpi" gap:platform="android" src="res/icon/android/icon.png" />
    <icon gap:platform="ios" height="57" src="res/icon/ios/icon.png" width="57" />
    <icon gap:platform="ios" height="72" src="res/icon/ios/icon.png" width="72" />
    <icon gap:platform="ios" height="114" src="res/icon/ios/icon.png" width="114" />
    <icon gap:platform="ios" height="144" src="res/icon/ios/icon.png" width="144" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="res/screen/splash.png" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="res/screen/splash.png" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="res/screen/splash.png" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="res/screen/splash.jpg" />
    <gap:splash gap:platform="ios" height="480" src="res/screen/splash.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="res/screen/splash.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="res/screen/splash.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="res/screen/splash.png" width="1024" />
    
    <access origin="http://127.0.0.1*" />
</widget>
